{
    "version": "1.0.0-*",

    "dependencies": {
        "Itinero": "1.0.0-*",
        "NETStandard.Library": "1.6.0"
    },

    "frameworks": {
        "net4.0": {
            "buildOptions": {
                "define": [ "NET40" ],
                "compile": {
                    "includeFiles": [
                        "../../SharedAssemblyVersion.cs"
                    ]
                }
            }
            
        },
        "net4.5": {
            "buildOptions": {
                "define": [ "NET45" ],
                "compile": {
                    "includeFiles": [
                        "../../SharedAssemblyVersion.cs"
                    ]
                }
            }
        },
        ".NETPortable,Version=v4.0,Profile=Profile147": {
            "buildOptions": {
                "define": [ "PORTABLE147" ],
                "compile": {
                    "includeFiles": [
                        "../../SharedAssemblyVersion.cs"
                    ]
                }
            },
            "frameworkAssemblies": {
                "System": ""
            }
        }
    }
}
