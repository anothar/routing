{
  "version": "1.0.0-*",
    "dependencies": {
        "Itinero": "1.0.0-*",
        "NETStandard.Library": "1.6.1",
        "OsmSharp": "6.0.0-alpha"
    },
  "frameworks": {
    "netstandard1.6": {
      "buildOptions": {
        "define": [ "NETSTANDARD" ],
        "compile": {
          "includeFiles": [
            "../../SharedAssemblyVersion.cs"
          ]
        },
        "embed": {
          "include": [ "Profiles/Defaults/*.lua" ]
        }
      },
      "imports": "dnxcore50",
        "dependencies": {
            "System.Xml.XmlSerializer": "4.3.0"
        }
    },
    "net4.0": {
      "buildOptions": {
        "define": [ "NET40" ],
        "compile": {
          "includeFiles": [
            "../../SharedAssemblyVersion.cs"
          ]
        },
        "embed": {
          "include": [ "Profiles/Defaults/*.lua" ]
        }
      },
      "frameworkAssemblies": {
        "System.Xml": ""
      }
    },
    "net4.5": {
      "buildOptions": {
        "define": [ "NET45" ],
        "compile": {
          "includeFiles": [
            "../../SharedAssemblyVersion.cs"
          ]
        },
        "embed": {
          "include": [ "Profiles/Defaults/*.lua" ]
        }
      },
      "frameworkAssemblies": {
        "System.Xml": ""
      }
    },
    ".NETPortable,Version=v4.0,Profile=Profile147": {
      "buildOptions": {
        "define": [ "PORTABLE147" ],
        "compile": {
          "includeFiles": [
            "../../SharedAssemblyVersion.cs"
          ]
        },
        "embed": {
          "include": [ "Profiles/Defaults/*.lua" ]
        }
      },
      "frameworkAssemblies": {
        "System": "",
        "System.Xml": "",
        "System.Xml.Serialization": ""
      },
      "dependencies": {
      }
    }
  }
}